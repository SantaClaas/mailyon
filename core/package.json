{
  "name": "@claas.dev/mailyon-core",
  "version": "0.0.0",
  "main": "index.js",
  "types": "index.d.ts",
  "napi": {
    "name": "mailyon-core",
    "triples": {
      "additional": [
        "aarch64-apple-darwin"
      ]
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@napi-rs/cli": "^2.16.3",
    "ava": "^5.1.1"
  },
  "ava": {
    "timeout": "3m"
  },
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "ensure-dist": "mkdir -p ./dist",
    "build": "npm run ensure-dist && napi build --platform --release --js ./dist/index.js --dts ./dist/index.d.ts",
    "build:debug": "npm run ensure-dist && napi build --platform --js ./dist/index.js --dts ./dist/index.d.ts",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "ava",
    "universal": "napi universal",
    "version": "napi version"
  }
}
